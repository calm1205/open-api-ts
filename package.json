{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "s2o": "swagger2openapi ./src/schema/swagger.json -o ./src/schema/openapi.json",
    "s2o-remote": "swagger2openapi http://localhost:3001/swagger.json -o ./src/schema/openapi.json",
    "schema": "npm run s2o && openapi-typescript ./src/schema/openapi.json -o ./src/schema/schema.d.ts",
    "schema-remote": "npm run s2o-remote && openapi-typescript ./src/schema/openapi.json -o ./src/schema/openapi-typescript/schema.d.ts",
    "schema-codegen": "npx openapi --input ./src/schema/openapi.json --output ./src/schema/openapi-typescript-codegen/schema.d.ts"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/node": "^20.12.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "openapi-fetch": "^0.9.3",
    "openapi-typescript": "~5.4.2",
    "openapi-typescript-codegen": "^0.29.0",
    "openapi-typescript-fetch": "^2.0.0",
    "swagger2openapi": "^7.0.8",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
